{
  "name": "cricket-game",
  "version": "1.0.0",
  "description": "Multiplayer desktop cricket game",
  "main": "public/electron.js",
  "author": "Cricket Game Developer",
  "license": "MIT",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run electron-start\"",
    "build": "npm run react-build && electron-builder",
    "react-start": "webpack serve --mode development",
    "react-build": "webpack --mode production",
    "electron-start": "cross-env NODE_ENV=development electron .",
    "electron-build": "electron-builder",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "keywords": ["cricket", "game", "electron", "react", "multiplayer"],
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.1.0",
    "@mui/material": "^7.1.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12",
    "pixi.js": "^8.9.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "socket.io-client": "^4.8.1",
    "wait-on": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "babel-loader": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron-forge": "^5.2.4",
    "electron-is-dev": "^3.0.1",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  }
}
